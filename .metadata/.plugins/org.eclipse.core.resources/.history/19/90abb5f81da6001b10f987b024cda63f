package com.RedSocial.core.converter;

import java.util.ArrayList;
import java.util.List;

import com.RedSocial.core.entity.Foto;
import com.RedSocial.core.entity.FotoJPG;
import com.RedSocial.core.model.FotoJPGModel;
import com.RedSocial.core.model.FotoModel;

public class FotoJPGConverter extends FotoConverter{

	@Override
	public List<FotoModel> convertir(List<Foto> fotos){
		
		List<FotoJPGModel> fotosModel = new ArrayList<>();
		
		for(Foto foto : fotos)
			fotosModel.add(new FotoJPGModel(foto));
		
		return fotosModel;
	}
}
