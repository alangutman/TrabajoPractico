package com.RedSocial.core.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

@Table(name="Foto")
@Entity
public interface Foto {
	
	@GeneratedValue
	@Id
	@Column(name="idFoto")
	public static final long idFoto = 0;
	
	public static final String nombre = "";
	
	public static final String ubicacion = "";
	
	public void mostrar();
}
